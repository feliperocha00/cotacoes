<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="extensao_form" model="ir.ui.view">
        <field name="name">extensao routes</field>
        <field name="model">routes</field>
        <field name="inherit_id" ref="routes.criacao_rotas_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='nome_rota']" position="after">
                <field name="validade"/>
            </xpath>
        </field>
    </record>
</odoo>